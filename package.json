    "ts-node": "^8.4.1",
    "typechain": "^1.0.3",
    "typechain-target-ethers": "^1.0.1",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build": "rimraf artifacts && rimraf typechain && npm run compile && npm run typechain && tsc",
    "compile": "npx buidler compile",
