{
  "name": "decentralized-booking-system",
  "version": "1.0.0",
  "description": "A completely decentralized application enabling companies to book spaces in a trustless fashion",
  "main": "dist/index.js",
  "dependencies": {},
  "devDependencies": {
    "@nomiclabs/buidler": "^1.0.1",
    "@nomiclabs/buidler-ethers": "^1.0.1",
    "@types/chai": "^4.2.4",
